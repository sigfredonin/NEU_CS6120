  (S
    (ADVP (RB Requirements))
    (NP (DT This) (NN help))
    (VP (VBZ is)
      (VP (VBN offered)
        (PP (TO to)
          (NP (NNS applicants)))
        (SBAR
          (SBAR
            (WHNP (WP who))
            (S
              (ADVP (RB ordinarily))
              (VP (MD would) (RB not)
                (VP (VB undertake)
                  (NP (DT the) (NN exploration))
                  (PP (IN under)
                    (NP (JJ present) (NNS conditions)
                      (CC or)
                      (NNS circumstances)))
                  (PP (IN at)
                    (NP (PRP$ their) (JJ sole) (NN expense)))))))
          (CC and)
          (SBAR
            (WHNP (WP who))
            (S
              (VP (VBP are)
                (ADJP (JJ unable)
                  (S
                    (VP (TO to)
                      (VP (VB obtain)
                        (NP (NNS funds))
                        (PP (IN from)
                          (NP
                            (NP (JJ commercial) (NNS sources))
                            (PP (IN on)
                              (NP (JJ reasonable) (NNS terms)))))))))))))))
    (. .)))

